Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.autohint:               false

URxvt.geometry:             120x30
URxvt.depth:                32
URxvt.transparent:          true
URxvt.fading:               0
URxvt.loginShell:           true
URxvt.saveLines:            12000
URxvt.internalBorder:       3
URxvt.lineSpace:            0

! Fonts
URxvt.allow_bold:           true
URxvt*font:                 xft:Hack:pixelsize=13
URxvt*boldFont:             xft:Hack:pixelsize=13

! Fix font space
URxvt*letterSpace:          -1

! Scrollbar
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

! Cursor
URxvt.cursorBlink:          false
URxvt*cursorColor:          #a3adb8
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true


! Terminal Colors
*fadeColor:                 #002b36
*pointerColorBackground:    #586e75
*pointerColorForeground:    #93a1a1
*foreground:                #a3adb8
*background:                rgba:3d00/2300/3e00/e600

! Black
*color0:                    #393939
*color8:                    #6a6a6a
! Red
*color1:                    #8f3b1b
*color9:                    #aa3328
! Green
*color2:                    #209850
*color10:                   #65bf68
! Yellow
*color3:                    #d8d400
*color11:                   #f8e502
#dbca69
! Blue
*color4:                    #203090
*color12:                   #4050f0
! Magenta
*color5:                    #7f448f
*color13:                   #af33bf
! Cyan
*color6:                    #5088bf
*color14:                   #30b9bf
! White
*color7:                    #a0a0a0
*color15:                   #e0e0e0
